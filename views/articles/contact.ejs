</div>

      <div  class="container-fluid">
        <div style="background-color: white; " class="row">
        
        <div  class="col col-lg-6 col-xl-6 col-md-12 col-sm-12 col-12" >
    
    
    
    
     
        <!-- end of services  class="section-center clearfix" -->
        <!-- contact -->
        <section class="contact">
        <div class="">
          <!-- contact info -->
          <article class="contact-info">
            <!-- contact item -->
            <div class="contact-item">
              <h3 class="contact-title">
                <span class="contact-icon"
                  ><i class="fas fa-location-arrow"></i></span
                >address
              </h3>
              <h3 class="contact-text">
                524,Gulshan-e-Ravi, Lahore
              </h3>
            </div>
            <!--end of  contact item -->
            <!-- contact item -->
            <div class="contact-item">
              <h3 class="contact-title">
                <span class="contact-icon"><i class="fas fa-envelope"></i></span
                >email
              </h3>
              <h3 class="contact-text">haristallat2001@gmail.com</h3>
            </div>
            <!--end of  contact item -->
            <!-- contact item -->
            <div class="contact-item">
              <h3 class="contact-title">
                <span class="contact-icon"><i class="fas fa-phone"></i></span
                >phone
              </h3>
              <h3 class="contact-text">03054425819</h3>
            </div>
            <!--end of  contact item -->
          </article>
        
          
        </div>
        
        
    
    
      </div>
        
        
        
        
      <div class="col col-lg-6 col-xl-6 col-md-12 col-sm-12 col-12">
    
    
        <!-- contact form -->
        <article class="contact-form">
          <form
            action="https://formspree.io/learncodetutorial@gmail.com"
            method="POST"
            class="form-group"
          >
            <input
              type="text"
              name="name"
              placeholder="name"
              class="form-control"
            />
            <input
              type="email"
              name="email"
              placeholder="email"
              class="form-control"
            />
            <textarea
              name="message"
              rows="5"
              class="form-control"
              placeholder="message"
            ></textarea>
            <button type="button" class="btn btn-rgb(173, 7, 7)">SUBMIT</button>
          </form>
        </article>
      </div>
     
    </div>
